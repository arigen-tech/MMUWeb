	<queryString><![CDATA[SELECT * 
FROM
(        
SELECT AKS.FROMDATE,AKS.TODATE,AKS.APPOINTMENT_TYPE_NAME,AKS.LevelHeading,AKS.HOSPITALNAME FROM (
SELECT   DISTINCT $P{FROM_DATE}   FROMDATE,  $P{TO_DATE}    TODATE  ,MAT.APPOINTMENT_TYPE_NAME , VT.VISIT_ID,
        (CASE WHEN 0=0  THEN ''  ELSE 'DISPOSAL TYPE: ' ||(SELECT MDL.DISPOSAL_NAME FROM MAS_DISPOSAL MDL where MDL.DISPOSAL_ID =0) || '   ' END ) ||
        (CASE WHEN 0=0  THEN ''  ELSE 'DEPARTMENT NAME: ' || MD.DEPARTMENT_NAME || '   '  END ) ||
        (CASE WHEN 0=0 THEN ''  ELSE 'DIAGONOSYS NAME: ' ||(SELECT ICD.ICD_NAME FROM MAS_ICD ICD where ICD.ICD_ID =0)|| '   '  END ) LevelHeading ,
	(SELECT HOSPITAL_NAME FROM MAS_HOSPITAL WHERE HOSPITAL_ID=$P{HOSPITAL_ID}) AS HOSPITALNAME 
FROM    VISIT VT
	LEFT OUTER JOIN PATIENT PT on PT.PATIENT_ID=VT.PATIENT_ID
        LEFT OUTER JOIN MAS_APPOINTMENT_TYPE MAT on MAT.APPOINTMENT_TYPE_ID=VT.APPOINTMENT_TYPE_ID
        LEFT OUTER JOIN OPD_DISPOSAL_DETAILS  OPD on OPD.VISIT_ID=VT.VISIT_ID
        LEFT OUTER JOIN MAS_DEPARTMENT MD on MD.DEPARTMENT_ID=VT.DEPARTMENT_ID
        LEFT OUTER JOIN DISCHARGE_ICD_CODE DIC on DIC.VISIT_ID=VT.VISIT_ID
        LEFT OUTER JOIN MAS_ICD ICD on ICD.ICD_ID=DIC.ICD_ID
WHERE   TO_DATE(TO_CHAR(CAST( VT.VISIT_DATE As Date))) BETWEEN TO_CHAR(to_Date($P{FROM_DATE} , 'DD-MM-YY')  )   AND TO_CHAR(to_Date($P{TO_DATE} , 'DD-MM-YY'))      AND
        VT.APPOINTMENT_TYPE_ID IS NOT NULL AND
        ( VT.APPOINTMENT_TYPE_ID =$P{APPOINTMENT_TYPE_ID} OR 0=$P{APPOINTMENT_TYPE_ID}) AND
	(OPD.DISPOSAL_ID=$P{DISPOSAL_ID} OR 0 = $P{DISPOSAL_ID}) AND
        (VT.DEPARTMENT_ID=$P{DEPARTMENT_ID} OR 0 = $P{DEPARTMENT_ID}) AND
        VT.HOSPITAL_ID=$P{HOSPITAL_ID}  AND	        
	( PT.SERVICE_NO=upper($P{SERVICE_NO}) OR '0'=upper($P{SERVICE_NO})) AND
        (DIC.ICD_ID=$P{ICD_ID} OR 0=$P{ICD_ID})  AND
        UPPER(VT.VISIT_STATUS)='C'  AND UPPER(VT.VISIT_FLAG)<>'E' 
)AKS)
PIVOT
(
   count(APPOINTMENT_TYPE_NAME) for  APPOINTMENT_TYPE_NAME IN('OPD','ME','MB')  
)]]></queryString>

