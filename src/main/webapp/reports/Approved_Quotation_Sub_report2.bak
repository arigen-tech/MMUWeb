	<parameter name="QUOTATION_M_ID" isForPrompting="true" class="java.lang.Integer">
		<defaultValueExpression ><![CDATA[new Integer(0)]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[SELECT COALESCE( NOMENCLATURE,'')||'('||COALESCE(PVMS_NO,'') ||')', MAS_STORE_UNIT.STORE_UNIT_NAME , 
STORE_QUOTATION_T.QTY_REQUIRED , STORE_BUDGETARY_T.LAST_LP_RATE , STORE_QUOTATION_T.UNIT_RATE , STORE_QUOTATION_T.TOTAL_COST,
SQM.HOSPITAL_ID
FROM ship.STORE_QUOTATION_M SQM
LEFT OUTER JOIN ship.STORE_QUOTATION_T ON STORE_QUOTATION_T.QUOTATION_M_ID=SQM.QUOTATION_M_ID
LEFT OUTER JOIN ship.STORE_BUDGETARY_M ON STORE_BUDGETARY_M.BUDGETARY_M_ID=SQM.BUDGETARY_M_ID
LEFT OUTER JOIN ship.STORE_BUDGETARY_T ON STORE_BUDGETARY_T.BUDGETARY_M_ID=SQM.BUDGETARY_M_ID 
                                     AND STORE_QUOTATION_T.ITEM_ID=STORE_BUDGETARY_T.ITEM_ID
LEFT OUTER JOIN ship.MAS_STORE_SUPPLIER ON MAS_STORE_SUPPLIER.SUPPLIER_ID=SQM.SUPPLIER_ID
LEFT OUTER JOIN ship.MAS_STORE_ITEM ON MAS_STORE_ITEM.ITEM_ID=STORE_QUOTATION_T.ITEM_ID
LEFT OUTER JOIN ship.MAS_STORE_UNIT ON MAS_STORE_UNIT.STORE_UNIT_ID=MAS_STORE_ITEM.DISP_UNIT_ID
WHERE SQM.QUOTATION_M_ID=$P{QUOTATION_M_ID}]]></queryString>

