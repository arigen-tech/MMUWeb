	<queryString><![CDATA[SELECT MAS_HOSPITAL.HOSPITAL_NAME , MAS_DEPARTMENT.DEPARTMENT_NAME , PATIENT.PATIENT_NAME , PATIENT.EMPLOYEE_NAME , DG_ORDER_HD.ORDER_NO , 
DG_ORDER_HD.ORDER_DATE ,VU_MAS_RELATION.RELATION_NAME ,R1.RANK_NAME , PATIENT.SERVICE_NO ,
floor(months_between(trunc(VISIT.VISIT_DATE),PATIENT.DATE_OF_BIRTH)/12)||' / '||VU_MAS_ADMINISTRATIVE_SEX.ADMINISTRATIVE_SEX_NAME as Gender, 
MAS_DEPARTMENT.DEPARTMENT_NAME||'/'||R2.RANK_NAME||' '||USERS.FIRST_NAME as Doctor_name,DG_RESULT_ENTRY_HD.RESULT_ENTRY_HD_ID
FROM DG_ORDER_HD
LEFT OUTER JOIN MAS_HOSPITAL ON DG_ORDER_HD.HOSPITAL_ID=MAS_HOSPITAL.HOSPITAL_ID
LEFT OUTER JOIN MAS_DEPARTMENT ON DG_ORDER_HD.DEPARTMENT_ID=MAS_DEPARTMENT.DEPARTMENT_ID
LEFT OUTER JOIN PATIENT ON DG_ORDER_HD.PATIENT_ID=PATIENT.PATIENT_ID
LEFT OUTER JOIN VISIT ON DG_ORDER_HD.VISIT_ID=VISIT.VISIT_ID
LEFT OUTER JOIN VU_MAS_RELATION ON PATIENT.RELATION_ID=VU_MAS_RELATION.RELATION_ID
LEFT OUTER JOIN VU_MAS_ADMINISTRATIVE_SEX ON PATIENT.ADMINISTRATIVE_SEX_ID=VU_MAS_ADMINISTRATIVE_SEX.ADMINISTRATIVE_SEX_ID
LEFT OUTER JOIN USERS ON DG_ORDER_HD.DOCTOR_ID=USERS.USER_ID
LEFT OUTER JOIN VU_MAS_EMPLOYEE on VU_MAS_EMPLOYEE.SERVICE_NO=USERS.SERVICE_NO
LEFT OUTER JOIN VU_MAS_RANK R2 on R2.RANK_CODE=VU_MAS_EMPLOYEE.RANK_ID
LEFT OUTER JOIN VU_MAS_RANK R1 on PATIENT.RANK_ID=R1.RANK_ID
LEFT OUTER JOIN MAS_DEPARTMENT ON MAS_DEPARTMENT.DEPARTMENT_ID=VISIT.DEPARTMENT_ID
LEFT OUTER JOIN DG_RESULT_ENTRY_HD ON DG_RESULT_ENTRY_HD.ORDERHD_ID=DG_ORDER_HD.ORDERHD_ID
where DG_ORDER_HD.ORDERHD_ID=$P{ORDERHD_ID}]]></queryString>

