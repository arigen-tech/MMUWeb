	<queryString><![CDATA[SELECT NURSING_NAME AS NAME, MF.FREQUENCY_NAME AS FREQUENCY,DT.NO_OF_DAYS ,DT.REMARKS
FROM ship.PROCEDURE_DT DT
LEFT OUTER JOIN ship.PROCEDURE_HD HD ON DT.PROCEDURE_HD_ID=HD.PROCEDURE_HD_ID
LEFT OUTER JOIN ship.MAS_NURSING_CARE MNC ON DT.PROCEDURE_ID=MNC.NURSING_ID
LEFT OUTER JOIN ship.MAS_FREQUENCY MF ON MF.FREQUENCY_ID=DT.FREQUENCY_ID
WHERE HD.VISIT_ID=$P{VISIT_ID} and NURSING_TYPE='N'
ORDER BY NURSING_NAME]]></queryString>

