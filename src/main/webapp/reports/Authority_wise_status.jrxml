	<queryString><![CDATA[SELECT 	VIA.authority_name,
		VIA.authority_date,
		VIA.authority_remarks,
		VIA.authority_action,
		STRING_AGG(MM.MMU_NAME, ', ') MMU,
		MD.UPSS UPSS,
		MMV.mmu_Vendor_name Vendor
from 	vendor_invoice_approval VIA 
left outer join capture_vendor_bill_detail CVBD on CVBD.capture_vendor_bill_detail_id = VIA.capture_vendor_bill_detail_id
left outer join capture_vendor_bill_mmu_detail CVBMD on CVBMD.capture_vendor_bill_detail_id = CVBD.capture_vendor_bill_detail_id
left outer join mas_authority MA on MA.authority_id = VIA.authority_id
left outer join mas_mmu_Vendor MMV on MMV.mmu_Vendor_id = CVBD.vendor_id
left outer join mas_district MD on CVBD.district_id = MD.district_id
left outer join MAS_MMU MM on MM.MMU_ID = CVBMD.MMU_ID
Where CVBMD.capture_vendor_bill_detail_id=$P{id}
GROUP BY VIA.authority_name,
		VIA.authority_date,
		VIA.authority_remarks,
		VIA.authority_action,
		MD.UPSS ,
		MMV.mmu_Vendor_name]]></queryString>

