	<queryString><![CDATA[SELECT PT.SERVICE_NO , PT.PATIENT_ID , MAS_HOSPITAL.HOSPITAL_NAME as mh1,
HVR.HOSPITAL_VISIT_DATE ,HVR.HOSTPITAL_VISIT_REGISTER_ID
FROM ship.HOSPITAL_VISIT_REGISTER HVR
LEFT OUTER JOIN ship.PATIENT PT ON PT.PATIENT_ID=HVR.PATIENT_ID
LEFT OUTER JOIN ship.MAS_HOSPITAL ON MAS_HOSPITAL.HOSPITAL_ID=HVR.HOSPITAL_ID
WHERE  HVR.HOSPITAL_VISIT_DATE  BETWEEN $P{FromDate} AND $P{ToDate}
AND HVR.HOSPITAL_ID=$P{HOSPITAL_ID} AND (upper(PT.SERVICE_NO)=upper($P{SERVICE_NO})  OR upper($P{SERVICE_NO})= ('0'))  $P!{query}
ORDER BY HOSPITAL_VISIT_DATE DESC]]></queryString>

