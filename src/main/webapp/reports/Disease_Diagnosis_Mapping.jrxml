	<queryString><![CDATA[select MAS_DISEASE.DISEASE_NAME,
AKS1.DISP,
(case when Upper(mas_disease_mapping.STATUS)='Y' then 'Active' else 'Inactive' end) STATUS
from ship.mas_disease_mapping 
LEFT OUTER JOIN MAS_DISEASE ON MAS_DISEASE.DISEASE_ID=mas_disease_mapping.DISEASE_ID
LEFT OUTER JOIN (SELECT RTRIM (XMLAGG (XMLELEMENT (E, ICD_NAME || ',')).EXTRACT ('//text()'), ',') Disp, DISEASE_MAPPING_ID
FROM (SELECT Distinct AA.DISEASE_MAPPING_ID,trim(regexp_substr(AA.ICD_ID, '[^,]+', 1, LEVEL)) str
FROM ship.mas_disease_mapping AA
CONNECT BY instr(AA.ICD_ID, ',', 1, LEVEL - 1) > 0)AKS,MAS_ICD
WHERE AKS.str=MAS_ICD.ICD_ID
GROUP BY DISEASE_MAPPING_ID) AKS1 on AKS1.DISEASE_MAPPING_ID=mas_disease_mapping.DISEASE_MAPPING_ID
order by MAS_DISEASE.DISEASE_NAME]]></queryString>

