	<queryString><![CDATA[SELECT AKS.E_HOS_ID, AKS.E_HOS_NAME,AKS.ADDRESS,AKS.PHONE_NO,case when upper (AKS.STATUS)='Y' 
then 'Active' else 'Inactive' end STATUS,
string_agg(MH.HOSPITAL_NAME ,', '  order by MH.HOSPITAL_NAME) HOSPITAL_NAME
FROM
(SELECT EMPANELLED_HOSPITAL_ID E_HOS_ID, EMPANELLED_HOSPITAL_NAME E_HOS_NAME,
  ADDRESS,PHONE_NO,STATUS,
    CAST(regexp_split_to_table(HOSPITAL_ID, E',') AS INTEGER) HOS_ID
FROM ship.MAS_EMPANELLED_HOSPITAL )AKS,ship.MAS_HOSPITAL MH
WHERE     AKS.HOS_ID=MH.HOSPITAL_ID
GROUP BY AKS.E_HOS_ID, AKS.E_HOS_NAME,AKS.ADDRESS,AKS.PHONE_NO,AKS.STATUS]]></queryString>

