	<queryString><![CDATA[SELECT  PT.PATIENT_ID AS DOOCUMENT_ID,
        TO_CHAR(CURRENT_DATE, 'DD/MM/YYYY') AS GENERATED_DATE,
        TO_CHAR(CURRENT_DATE,'HH24:MI') AS GENERATED_TIME,
        (SELECT FIRST_NAME FROM ship.USERS WHERE USER_ID=$P{P_USERID}) AS USER_NAME,
        (SELECT HOSPITAL_NAME FROM ship.MAS_HOSPITAL WHERE HOSPITAL_ID = (SELECT HOSPITAL_ID FROM ship.USERS WHERE USER_ID=$P{P_USERID})) AS LOCATION,
        PT.EMPLOYEE_NAME AS EMPLOYEE_NAME,
        PT.SERVICE_NO AS SERVICE_NO,
        PT.PATIENT_NAME AS PATIENT_NAME,
        PT.UHID_NO AS UHID_NO,
        MR.RELATION_NAME AS RELATION_NAME,
        MST.SERVICE_TYPE_NAME  SERVICE_TYPE,
        MAS.ADMINISTRATIVE_SEX_NAME AS GENDER,
        MT.TRADE_NAME AS TRADE,
        TO_CHAR(PT.DATE_OF_BIRTH, 'DD/MM/YYYY') AS DOB,
        AGE(CURRENT_DATE,PT.DATE_OF_BIRTH) AS AGE,
        MMS.MARITAL_STATUS_NAME AS MARITAL_STATUS,
        RELIGION_NAME AS RELIGION,
        PT.EMAIL_ID AS EMAIL_ID,
        COALESCE(PT.ADR1,'')||(CASE WHEN PT.ADR2 IS NULL THEN COALESCE(PT.ADR2,'') ELSE ', '||COALESCE(PT.ADR2,'') END)||
                         (CASE WHEN PT.ADR3 IS NULL THEN COALESCE(PT.ADR3,'') ELSE ', '||COALESCE(PT.ADR3,'') END)||
                         (CASE WHEN PT.ADR4 IS NULL THEN COALESCE(PT.ADR4,'') ELSE ', '||COALESCE(PT.ADR4,'') END)||
                         (CASE WHEN PINCODE IS NULL THEN '' ELSE ' -'||CAST(PINCODE AS VARCHAR) END)||
                         (CASE WHEN DISTRICT_NAME IS NULL THEN COALESCE(DISTRICT_NAME,'') ELSE ', '||COALESCE(DISTRICT_NAME,'') END)||
                         (CASE WHEN STATE_NAME IS NULL THEN COALESCE(STATE_NAME,'') ELSE ', '||COALESCE(STATE_NAME,'') END)||
                         ',INDIA'  AS ADDRESS,
        PT.MOBILE_NUMBER AS MOBILE_NUMBER,
        PT.NOK1_NAME AS NOK1_NAME,
        MRN.RELATION_NAME AS NOK1_RELATION,
        COALESCE(PT.NOK1_ADR1,'')||','||COALESCE(PT.NOK1_ADR2,'')||','||COALESCE(PT.NOK1_ADR3,'')||','||COALESCE(PT.NOK1_ADR4,'')AS NOK1_ADDRESS,
        PT.NOK1_MOBILE_NO AS NOK1_MOBILE_NO,
        MBG.BLOOD_GROUP_NAME AS BLOOD_GROUP ,
        (SELECT string_agg(OPH.ALLERGY_HISTORY::text, ',')  FROM ship.OPD_PATIENT_HISTORY OPH WHERE OPH.PATIENT_ID=PT.PATIENT_ID and ALLERGY_HISTORY IS NOT NULL) AS ALLERGY_ALERTS,
        (SELECT string_agg(OPH.PAST_SURGICAL_HISTORY::text, ',')  FROM ship.OPD_PATIENT_HISTORY OPH WHERE OPH.PATIENT_ID=PT.PATIENT_ID and PAST_SURGICAL_HISTORY IS NOT NULL) AS PAST_SURGERIES,
        (SELECT string_agg(OPH.IMPLANT_HISTORY::text, ',')  FROM ship.OPD_PATIENT_HISTORY OPH WHERE OPH.PATIENT_ID=PT.PATIENT_ID and IMPLANT_HISTORY IS NOT NULL) AS IMPLANT_HISTORY,
        '' AS IMMUNIZATIONS,
        '' AS MOST_RECENT_VITAL_SIGNS,
        '' AS MOST_RECENT_LAB_RESULTS,
        '' AS MOST_RECENT_IMAGING_RESULTS,
        (SELECT string_agg(OPD.ICD_DIAGNOSIS::text, ',')  FROM ship.OPD_PATIENT_DETAILS OPD WHERE   OPD.PATIENT_ID=$P{P_PATIENTID} and ICD_DIAGNOSIS IS NOT NULL) AS CURRENT_MEDICAL_PROBLEMS,
        '' AS CURRENT_MEDICATIONS
FROM    ship.PATIENT PT
        LEFT OUTER JOIN ship.VU_MAS_RELATION MR ON MR.RELATION_ID=PT.RELATION_ID
        LEFT OUTER JOIN ship.MAS_SERVICE_TYPE MST ON MST.SERVICE_TYPE_ID=PT.SERVICE_STATUS_ID
        LEFT OUTER JOIN ship.MAS_ADMINISTRATIVE_SEX MAS ON MAS.ADMINISTRATIVE_SEX_ID=PT.ADMINISTRATIVE_SEX_ID
        LEFT OUTER JOIN ship.VU_MAS_TRADE MT ON MT.TRADE_ID=PT.TRADE_ID
        LEFT OUTER JOIN ship.VU_MAS_MARITAL_STATUS MMS ON MMS.MARITAL_STATUS_ID=PT.MARITAL_STATUS_ID
        LEFT OUTER JOIN ship.MAS_RELIGION MRP ON MRP.RELIGION_ID=PT.RELIGION_ID
        LEFT OUTER JOIN ship.VU_MAS_RELATION MRN ON MRN.RELATION_ID=PT.NOK1_RELATION_ID
        LEFT OUTER JOIN ship.MAS_BLOOD_GROUP MBG ON MBG.BLOOD_GROUP_ID=PT.BLOOD_GROUP_ID
        LEFT OUTER JOIN ship.VU_MAS_STATE VMS  ON VMS.STATE_ID=PT.STATE_ID
        LEFT OUTER JOIN ship.VU_MAS_DISTRICT VMD  ON VMD.DISTRICT_ID=VMS.STATE_ID
WHERE   PATIENT_ID=$P{P_PATIENTID}]]></queryString>

