	<queryString><![CDATA[SELECT  pvms_no,
        mas.NOMENCLATURE,
        msu.store_unit_name,
        CLOSING_STOCK,
        rol_d,
        aks.Hospital_name,
        aks.department_name
from    ship.mas_store_item mas, (        
select  msi.item_id,
        sum(Bat.CLOSING_STOCK) as CLOSING_STOCK,
        Hospital_name,
        department_name
from    ship.STORE_ITEM_BATCH_STOCK Bat
        left outer join ship.mas_store_item msi   on msi.item_id=Bat.item_id 
        left outer join ship.MAS_HOSPITAL MH on MH.HOSPITAL_id=Bat.HOSPITAL_id 
        left outer join ship.MAS_department MD on MD.department_id=Bat.department_id 
where   Bat.HOSPITAL_ID=$P{HOSPITAL_ID} and Bat.DEPARTMENT_ID=$P{DEPARTMENT_ID}         
group by msi.item_id,Hospital_name,
        department_name)aks,mas_store_unit msu  
where aks.item_id=mas.item_id and
      mas.item_unit_id=  msu.store_unit_id and 
      mas.rol_d >= aks.CLOSING_STOCK]]></queryString>

