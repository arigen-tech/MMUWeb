	<queryString><![CDATA[SELECT 'Dispensary - Prescription Slip - '|| MAS_MMU.MMU_NAME HOSPITAL_NAME ,PATIENT.PATIENT_NAME ,  PATIENT_PRESCRIPTION_HD.VISIT_ID, PT_AGE(PATIENT.DATE_OF_BIRTH,VISIT.VISIT_DATE) AGE ,
PATIENT_PRESCRIPTION_HD.DISPENSARY_ISSUE_NO , PATIENT_PRESCRIPTION_HD.PRESCRIPTION_DATE,U1.USER_NAME ||' ('||ut1.user_type_name||')' as DispensaryAssistant,
'Dr. ' ||U2.USER_NAME||CASE WHEN E1.reg_no IS NULL THEN '' ELSE '( ' END ||COALESCE(E1.reg_no,'')||
CASE WHEN E1.reg_no IS NULL THEN '' ELSE ') ' END as DoctorName,OPD_PATIENT_DETAILS.WORKING_DIAGNOSIS
FROM PATIENT_PRESCRIPTION_HD
LEFT OUTER JOIN PATIENT ON PATIENT.PATIENT_ID=PATIENT_PRESCRIPTION_HD.PATIENT_ID
LEFT OUTER JOIN VISIT ON VISIT.VISIT_ID=PATIENT_PRESCRIPTION_HD.VISIT_ID
LEFT OUTER JOIN MAS_MMU on MAS_MMU.MMU_ID=PATIENT_PRESCRIPTION_HD.MMU_ID
LEFT OUTER JOIN USERS U1 ON U1.USER_ID=PATIENT_PRESCRIPTION_HD.ISSUED_BY
LEFT OUTER JOIN MAS_USER_TYPE ut1 on ut1.user_type_id=u1.user_type_id
LEFT OUTER JOIN USERS U2 on PATIENT_PRESCRIPTION_HD.DOCTOR_ID=U2.USER_ID
LEFT OUTER JOIN MAS_EMPLOYEE E1 on E1.EMPLOYEE_ID=U2.EMPLOYEE_ID
LEFT OUTER JOIN OPD_PATIENT_DETAILS ON OPD_PATIENT_DETAILS.OPD_PATIENT_DETAILS_ID=PATIENT_PRESCRIPTION_HD.OPD_PATIENT_DETAILS_ID
WHERE PATIENT_PRESCRIPTION_HD.PRESCRIPTION_HD_ID is not null and PATIENT_PRESCRIPTION_HD.PRESCRIPTION_HD_ID=$P{PRESCRIPTION_HD_ID}]]></queryString>

