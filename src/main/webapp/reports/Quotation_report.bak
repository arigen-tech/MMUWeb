	</parameter>
	<parameter name="QUOTATION_M_ID" isForPrompting="true" class="java.lang.Integer">
		<defaultValueExpression ><![CDATA[new Integer(0)]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[SELECT MAS_HOSPITAL.HOSPITAL_NAME,MAS_DEPARTMENT.DEPARTMENT_NAME, STORE_BUDGETARY_M.REQ_NO  , STORE_BUDGETARY_M.REQ_DATE , 
R1.RANK_NAME ||' '|| U1.FIRST_NAME AS REQUESTED_BY  , MAS_STORE_SUPPLIER.SUPPLIER_NAME , STORE_QUOTATION_M.QUOTATION_NO ,
COALESCE( NOMENCLATURE,'')||'('||COALESCE(PVMS_NO,'') ||')', MAS_STORE_UNIT.STORE_UNIT_NAME , STORE_QUOTATION_T.QTY_REQUIRED , 
STORE_BUDGETARY_T.LAST_LP_RATE , STORE_QUOTATION_T.UNIT_RATE , STORE_QUOTATION_T.TOTAL_COST,
R2.RANK_NAME ||' '|| U2.FIRST_NAME AS CREATED_BY , REMARKS ,(case upper (STORE_BUDGETARY_M.STATUS) when 'Y' then 'Created'
                                        when 'M' then 'Approved'              
                                        WHEN 'K' THEN 'Rejected'
                                        end) As STATUS ,
R3.RANK_NAME ||' '|| U3.FIRST_NAME AS APPROVED_BY , APPROVED_DATE,
(case upper (STORE_QUOTATION_M.STATUS) when  'M' then 'Approved By '
                                        WHEN 'K' THEN 'Rejected By'
                                        else '' end) As BYNAME,
(case upper (STORE_QUOTATION_M.STATUS) when  'M' then 'Approved Date'              
                                        WHEN 'K' THEN 'Rejected Date'
                                        else '' end) As BYDATE
FROM ship.STORE_QUOTATION_M
LEFT OUTER JOIN ship.STORE_QUOTATION_T ON STORE_QUOTATION_T.QUOTATION_M_ID=STORE_QUOTATION_M.QUOTATION_M_ID
LEFT OUTER JOIN ship.STORE_BUDGETARY_M ON STORE_BUDGETARY_M.BUDGETARY_M_ID=STORE_QUOTATION_M.BUDGETARY_M_ID
LEFT OUTER JOIN ship.STORE_BUDGETARY_T ON STORE_BUDGETARY_T.BUDGETARY_M_ID=STORE_QUOTATION_M.BUDGETARY_M_ID 
                                     AND STORE_QUOTATION_T.ITEM_ID=STORE_BUDGETARY_T.ITEM_ID
LEFT OUTER JOIN ship.USERS U1 ON U1.USER_ID=STORE_BUDGETARY_M.REQ_BY
LEFT OUTER JOIN ship.VU_MAS_EMPLOYEE E1 on E1.SERVICE_NO=U1.SERVICE_NO
LEFT OUTER JOIN ship.VU_MAS_RANK R1 on R1.RANK_CODE=E1.RANK_ID
LEFT OUTER JOIN ship.MAS_STORE_SUPPLIER ON MAS_STORE_SUPPLIER.SUPPLIER_ID=STORE_QUOTATION_M.SUPPLIER_ID
LEFT OUTER JOIN ship.MAS_STORE_ITEM ON MAS_STORE_ITEM.ITEM_ID=STORE_QUOTATION_T.ITEM_ID
LEFT OUTER JOIN ship.MAS_STORE_UNIT ON MAS_STORE_UNIT.STORE_UNIT_ID=MAS_STORE_ITEM.DISP_UNIT_ID
LEFT OUTER JOIN ship.MAS_HOSPITAL ON MAS_HOSPITAL.HOSPITAL_ID=STORE_QUOTATION_M.HOSPITAL_ID
LEFT OUTER JOIN ship.MAS_DEPARTMENT ON MAS_DEPARTMENT.DEPARTMENT_ID=STORE_QUOTATION_M.DEPARTMENT_ID
LEFT OUTER JOIN ship.USERS U2 ON U2.USER_ID=STORE_QUOTATION_M.CREATED_BY
LEFT OUTER JOIN ship.VU_MAS_EMPLOYEE E2 on E2.SERVICE_NO=U2.SERVICE_NO
LEFT OUTER JOIN ship.VU_MAS_RANK R2 on R2.RANK_CODE=E2.RANK_ID
LEFT OUTER JOIN ship.USERS U3 ON U3.USER_ID=STORE_QUOTATION_M.APPROVED_BY
LEFT OUTER JOIN ship.VU_MAS_EMPLOYEE E3 on E3.SERVICE_NO=U3.SERVICE_NO
LEFT OUTER JOIN ship.VU_MAS_RANK R3 on R3.RANK_CODE=E3.RANK_ID
WHERE STORE_QUOTATION_M.QUOTATION_M_ID=$P{QUOTATION_M_ID}]]></queryString>

