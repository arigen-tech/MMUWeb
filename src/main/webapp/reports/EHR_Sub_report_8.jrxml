	<parameter name="P_VISITID" isForPrompting="true" class="java.lang.Integer">
		<defaultValueExpression ><![CDATA[new Integer(0)]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[SELECT  (CASE WHEN HD.TREATMENT_TYPE ='E' THEN  MEH.EMPANELLED_HOSPITAL_NAME ELSE MH.HOSPITAL_NAME END) HOSPITAL_NAME,
        (CASE WHEN HD.TREATMENT_TYPE ='E' THEN  DT.EXT_DEPARTMENT ELSE MD.DEPARTMENT_NAME END) Speciality,
        MI.ICD_NAME AS DIAGONSYS,
        DT.INSTRUCTION AS INSTRUCTION,
        VT.VISIT_ID
FROM public.REFERRAL_PATIENT_DT DT
LEFT OUTER JOIN public.REFERRAL_PATIENT_HD HD ON HD.REFREAL_HD_ID=DT.REFREAL_HD_ID
LEFT OUTER JOIN public.OPD_PATIENT_DETAILS OPD ON OPD.OPD_PATIENT_DETAILS_ID=HD.OPD_PATIENT_DETAILS_ID
LEFT OUTER JOIN public.VISIT VT ON VT.VISIT_ID=OPD.VISIT_ID
LEFT OUTER JOIN public.MAS_ICD MI ON MI.ICD_ID=DT.DIAGNOSIS_ID
LEFT OUTER JOIN public.MAS_DEPARTMENT MD ON MD.DEPARTMENT_ID=DT.INT_DEPARTMENT_ID
LEFT OUTER JOIN public.MAS_HOSPITAL MH ON MH.HOSPITAL_ID=HD.INT_HOSPITAL_ID
LEFT OUTER JOIN public.MAS_EMPANELLED_HOSPITAL MEH ON MEH.EMPANELLED_HOSPITAL_ID=HD.EXT_HOSPITAL_ID
WHERE    HD.PATIENT_ID=$P{P_PATIENTID} AND UPPER(VT.VISIT_STATUS)='C'  AND  VT.VISIT_ID =$P{P_VISITID}]]></queryString>

