	<parameter name="FromDate" isForPrompting="true" class="java.util.Date">
		<defaultValueExpression ><![CDATA[new Date()]]></defaultValueExpression>
	</parameter>
	<parameter name="ToDate" isForPrompting="true" class="java.util.Date">
		<defaultValueExpression ><![CDATA[new Date()]]></defaultValueExpression>
	</parameter>
	<parameter name="HOSPITAL_ID" isForPrompting="true" class="java.lang.Integer">
		<defaultValueExpression ><![CDATA[new Integer(0)]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[SELECT MAS_MEDICAL_CATEGORY.MEDICAL_CATEGORY_NAME , MAS_HOSPITAL.HOSPITAL_NAME ,COUNT(*)
FROM ship.PATIENT_MEDICAL_CAT
LEFT OUTER JOIN ship.VISIT ON VISIT.VISIT_ID=PATIENT_MEDICAL_CAT.VISIT_ID
LEFT OUTER JOIN ship.MAS_MEDICAL_CATEGORY ON MAS_MEDICAL_CATEGORY.MEDICAL_CATEGORY_ID=PATIENT_MEDICAL_CAT.MEDICAL_CATEGORY_ID
LEFT OUTER JOIN ship.MAS_HOSPITAL ON MAS_HOSPITAL.HOSPITAL_ID=VISIT.HOSPITAL_ID
LEFT OUTER JOIN ship.MAS_APPOINTMENT_TYPE ON MAS_APPOINTMENT_TYPE.APPOINTMENT_TYPE_ID=VISIT.APPOINTMENT_TYPE_ID
WHERE  CAST( VISIT.VISIT_DATE As Date)  BETWEEN $P{FromDate}   AND $P{ToDate} 
AND (VISIT.HOSPITAL_ID=$P{HOSPITAL_ID} OR '0'=$P{HOSPITAL_ID}) AND 
upper(VISIT.VISIT_STATUS)='C' AND upper(VISIT.EXAM_STATUS)='C' AND upper(VISIT.VISIT_FLAG)='R' 
AND upper(MAS_APPOINTMENT_TYPE.APPOINTMENT_TYPE_CODE)='ME' 
AND PATIENT_MEDICAL_CAT.MEDICAL_CATEGORY_ID is not null
GROUP BY MAS_MEDICAL_CATEGORY.MEDICAL_CATEGORY_NAME ,MAS_HOSPITAL.HOSPITAL_NAME
ORDER BY MAS_MEDICAL_CATEGORY.MEDICAL_CATEGORY_NAME]]></queryString>

