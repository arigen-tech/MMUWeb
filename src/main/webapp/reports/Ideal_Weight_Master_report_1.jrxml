	<queryString><![CDATA[SELECT   MAS.ADMINISTRATIVE_SEX_NAME , A.FROM_RANGE As FROM_AGE, A.TO_RANGE As  TO_AGE, H.FROM_RANGE As FROM_HEIGHT, H.TO_RANGE As To_HEIGHT , MIW.WEIGHT ,  
        (CASE WHEN UPPER(MIW.STATUS)='Y'  THEN 'Active' ELSE 'Inactive' end ) as STATUS 
FROM    ship.MAS_IDEAL_WEIGHT MIW
LEFT OUTER JOIN ship.MAS_ADMINISTRATIVE_SEX  MAS ON MAS.ADMINISTRATIVE_SEX_ID=MIW.GENDER_ID
LEFT OUTER JOIN ship.MAS_RANGE A on  A.RANGE_id =  MIW.AGE_RANGE_ID and A.range_Flag='A' AND A.GENDER_ID=MIW.GENDER_ID
LEFT OUTER JOIN ship.MAS_RANGE H on  H.RANGE_id =  MIW.HEIGHT_RANGE_ID  and H.range_Flag='H' AND H.GENDER_ID=MIW.GENDER_ID
ORDER BY MAS.ADMINISTRATIVE_SEX_NAME,FROM_AGE,TO_AGE,FROM_HEIGHT,To_HEIGHT]]></queryString>

