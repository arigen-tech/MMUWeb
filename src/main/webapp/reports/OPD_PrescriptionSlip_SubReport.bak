	<parameter name="PRESCRIPTION_HD_ID" isForPrompting="true" class="java.lang.Integer">
		<defaultValueExpression ><![CDATA[new Integer(0)]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[SELECT  DT.ITEM_ID, MSI.NOMENCLATURE,DT.DOSAGE,MF.FREQUENCY_NAME,DT.NO_OF_DAYS,DT.TOTAL,DT.QTY_ISSUED,DT.INSTRUCTION,HD.VISIT_ID,
        HD.PRESCRIPTION_DATE,R.RANK_NAME||''||U.FIRST_NAME as FIRST_NAME ,HD.OTHER_TREATMENT
        FROM ship.PATIENT_PRESCRIPTION_DT DT  
        LEFT OUTER JOIN ship.MAS_STORE_ITEM MSI ON DT.ITEM_ID=MSI.ITEM_ID
        LEFT OUTER JOIN ship.PATIENT_PRESCRIPTION_HD HD ON DT.PRESCRIPTION_HD_ID=HD.PRESCRIPTION_HD_ID
        LEFT OUTER JOIN ship.MAS_FREQUENCY  MF ON MF.FREQUENCY_ID=DT.FREQUENCY_ID 
        LEFT OUTER JOIN (SELECT   T.PRESCRIPTION_DT_ID DT_ID ,T.ISSUE_BY
        FROM  ship.STORE_ISSUE_T T,PATIENT_PRESCRIPTION_DT IDT 
        WHERE T.PRESCRIPTION_DT_ID=IDT.PRESCRIPTION_DT_ID 
        GROUP BY T.PRESCRIPTION_DT_ID,T.ISSUE_BY ) T ON T.DT_ID=DT.PRESCRIPTION_DT_ID
        LEFT OUTER JOIN ship.USERS U ON T.ISSUE_BY=U.USER_ID   
        LEFT OUTER JOIN ship.VU_MAS_EMPLOYEE E on E.SERVICE_NO=U.SERVICE_NO
        LEFT OUTER JOIN ship.VU_MAS_RANK R on R.RANK_CODE=E.RANK_ID
        WHERE  DT.PRESCRIPTION_HD_ID=$P{PRESCRIPTION_HD_ID}
        GROUP BY DT.ITEM_ID, MSI.NOMENCLATURE,DT.DOSAGE,MF.FREQUENCY_NAME,DT.NO_OF_DAYS,DT.TOTAL,DT.QTY_ISSUED,DT.INSTRUCTION,HD.VISIT_ID,
        HD.PRESCRIPTION_DATE,R.RANK_NAME,U.FIRST_NAME,HD.OTHER_TREATMENT]]></queryString>

