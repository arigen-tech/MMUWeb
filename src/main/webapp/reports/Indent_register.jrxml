	<queryString><![CDATA[select indent_m.demand_no,indent_m.demand_date,MMU.mmu_name,city_name,u.user_name,
(CASE indent_m.status WHEN 'N' THEN  'Pending for submission'
                         WHEN 'Y' THEN  'Pending for APM'
                         WHEN 'A' THEN  'Pending at Doctor'
                         WHEN 'R' THEN  'Rejected by APM'
                         WHEN 'U' THEN  'Pending at CO'
                         WHEN 'X' THEN  'Rejected by CO'
                         WHEN 'C' THEN  'Approved'
                         WHEN 'Z' THEN  'Rejected by CO'
                         ELSE 'NA' END)status ,
(CASE indent_m.status WHEN 'N' THEN  ''
                         WHEN 'Y' THEN  ''
                         WHEN 'A' THEN  (SELECT user_name||' (APM)' FROM USERS WHERE USER_ID=apm_id)
                         WHEN 'R' THEN  ''
                         WHEN 'U' THEN  (SELECT user_name||' (Doctor)' FROM USERS WHERE USER_ID=auditor_id)
                         WHEN 'X' THEN  ''
                         WHEN 'C' THEN  (SELECT user_name||' (City officer)' FROM USERS WHERE USER_ID=Chmo_id)
                         WHEN 'Z' THEN  ''
                         ELSE 'NA' END)approved_by 
from store_internal_indent_m indent_m
INNER join mas_mmu MMU ON indent_m.mmu_id=MMU.mmu_id
INNER join CITY_MMU_MAPPING CMM ON CMM.mmu_id=MMU.mmu_id
INNER join mas_city city ON city.city_id=CMM.city_id and city.city_id=indent_m.city_id
left outer join users u ON indent_m.created_by=u.user_id
where indent_m.demand_date Between $P{Fromdate} AND  $P{Todate} AND
	 (indent_m.mmu_id = $P{Mmu_id} OR $P{Mmu_id}= 0) AND
indent_m.status not IN ('I','R')AND
	MMU.MMU_ID IN (
	  	SELECT 	MMU.MMU_ID
		FROM 	MAS_MMU MMU,CITY_MMU_MAPPING MCM,(SELECT  	to_number(foo,'99999999') IDS
				     FROM 	regexp_split_to_table((SELECT 	(CASE WHEN 'M' = $P{Level_of_user} THEN substring(M.MMU_ID, 1 ,length(M.MMU_ID)-1) 
										      WHEN 'C' = $P{Level_of_user} THEN substring(M.CITY_ID, 1 ,length(M.CITY_ID)-1) 
										      WHEN 'D' = $P{Level_of_user} THEN substring(M.DISTRICT_ID, 1 ,length(M.DISTRICT_ID)-1) 
										      ELSE '1' END)
									FROM 	USERS M 
									WHERE 	M.USER_ID =$P{User_id}), E',') As foo)  ALL_ID 
		WHERE MCM.MMU_ID = MMU.MMU_ID AND
			( (MMU.MMU_ID=ALL_ID.IDS AND 'M' =$P{Level_of_user}) OR
	  		(MCM.CITY_ID=ALL_ID.IDS AND 'C' =$P{Level_of_user}) OR
			(MMU.STATE_ID=ALL_ID.IDS AND 'S' =$P{Level_of_user}) OR
			(MMU.DISTRICT_ID=ALL_ID.IDS AND 'D' =$P{Level_of_user}) ) 
		)
ORDER BY MMU.mmu_name]]></queryString>

