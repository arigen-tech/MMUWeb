	<parameter name="P_PATIENTID" isForPrompting="true" class="java.lang.Integer">
		<defaultValueExpression ><![CDATA[new Integer(0)]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[SELECT ORDER_NO,INVESTIGATION_DATE,INVESTIGATION_TYPE,INVESTIGATION_NAME , AKS.R FROM (
SELECT  HD.ORDER_NO  AS ORDER_NO,
        HD.ORDER_DATE  AS INVESTIGATION_DATE,
        MMC.MAIN_CHARGECODE_NAME AS INVESTIGATION_TYPE,
        DMI.INVESTIGATION_NAME AS INVESTIGATION_NAME ,DG_RESULT_ENTRY_DT.RESULT as r,
        DENSE_RANK() OVER (PARTITION BY  DT.ORDERHD_ID ORDER BY  HD.ORDER_DATE  Desc)  AS AKS
FROM    ship.DG_ORDER_HD HD
        LEFT OUTER JOIN ship.DG_ORDER_DT DT   ON HD.ORDERHD_ID=DT.ORDERHD_ID 
        LEFT OUTER JOIN ship.DG_MAS_INVESTIGATION DMI ON DMI.INVESTIGATION_ID=DT.INVESTIGATION_ID 
        LEFT OUTER JOIN ship.MAS_MAIN_CHARGECODE MMC ON MMC.MAIN_CHARGECODE_ID=DMI.MAIN_CHARGECODE_ID 
        LEFT OUTER JOIN ship.DG_RESULT_ENTRY_DT ON DT.ORDERDT_ID=DG_RESULT_ENTRY_DT.ORDERDT_ID
WHERE   HD.PATIENT_ID=$P{P_PATIENTID} and   MMC.MAIN_CHARGECODE_ID=1 )AKS
WHERE   AKS.AKS >=1
ORDER BY INVESTIGATION_DATE DESC]]></queryString>

