	<queryString><![CDATA[SELECT distinct MAS_HOSPITAL.HOSPITAL_NAME ,DG_MAS_INVESTIGATION.INVESTIGATION_ID ,PATIENT.SERVICE_NO ,PATIENT.EMPLOYEE_NAME ,
VU_MAS_RANK.RANK_NAME ,VU_MAS_ADMINISTRATIVE_SEX.ADMINISTRATIVE_SEX_NAME,
age(current_date ,PATIENT.DATE_OF_BIRTH) AGE  ,  MAS_MED_EXAM.MEDICAL_EXAM_NAME ,r2.RANK_NAME   ||'   '|| U.FIRST_NAME|| ' ' ||U.LAST_NAME AS DOCTOR_NAME  ,
DG_ORDER_DT.INVESTIGATION_ID
FROM SHIP.PATIENT 
LEFT OUTER JOIN SHIP.VISIT on PATIENT.PATIENT_ID=VISIT.PATIENT_ID
LEFT OUTER JOIN SHIP.MAS_HOSPITAL on MAS_HOSPITAL.HOSPITAL_ID=VISIT.HOSPITAL_ID
LEFT OUTER JOIN SHIP.VU_MAS_RANK on VU_MAS_RANK.RANK_ID=PATIENT.RANK_ID
LEFT OUTER JOIN SHIP.VU_MAS_ADMINISTRATIVE_SEX on VU_MAS_ADMINISTRATIVE_SEX.ADMINISTRATIVE_SEX_ID=PATIENT.ADMINISTRATIVE_SEX_ID
LEFT OUTER JOIN SHIP.MAS_MED_EXAM on MAS_MED_EXAM.MEDICAL_EXAM_ID=VISIT.EXAM_TYPE
LEFT OUTER JOIN SHIP.DG_ORDER_HD on DG_ORDER_HD.VISIT_ID=VISIT.VISIT_ID
LEFT OUTER JOIN SHIP.USERS U on U.USER_ID=DG_ORDER_HD.DOCTOR_ID
LEFT OUTER JOIN SHIP.VU_MAS_EMPLOYEE on VU_MAS_EMPLOYEE.SERVICE_NO=U.SERVICE_NO
LEFT OUTER JOIN SHIP.VU_MAS_RANK r2 on r2.RANK_CODE=VU_MAS_EMPLOYEE.RANK_ID
LEFT OUTER JOIN SHIP.DG_ORDER_DT on DG_ORDER_DT.ORDERHD_ID=DG_ORDER_HD.ORDERHD_ID
LEFT OUTER JOIN SHIP.DG_MAS_INVESTIGATION on DG_MAS_INVESTIGATION.INVESTIGATION_ID=DG_ORDER_DT.INVESTIGATION_ID
WHERE VISIT.VISIT_ID=$P{VISIT_ID}]]></queryString>

