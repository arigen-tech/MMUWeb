	<queryString><![CDATA[Select  VT.VISIT_ID AS VISIT_ID,
        MSI.NOMENCLATURE AS ITEM_NAME,
        DT.Dosage AS DOSAGE,  
        MF.FREQUENCY_NAME AS FREQUENCY,
        DT.NO_OF_DAYS AS NO_OF_DAYS,
        HD.OTHER_TREATMENT
From    ship.PATIENT_PRESCRIPTION_DT DT
LEFT OUTER JOIN ship.PATIENT_PRESCRIPTION_HD HD ON HD.PRESCRIPTION_HD_ID=DT.PRESCRIPTION_HD_ID
LEFT OUTER JOIN ship.VISIT VT ON VT.VISIT_ID=HD.VISIT_ID
LEFT OUTER JOIN ship.MAS_STORE_ITEM MSI ON MSI.ITEM_ID=DT.ITEM_ID
LEFT OUTER JOIN ship.MAS_FREQUENCY MF ON MF.FREQUENCY_ID=DT.FREQUENCY_ID
WHERE   HD.PATIENT_ID=$P{P_PATIENTID} AND UPPER(VT.VISIT_STATUS)='C' AND  HD.VISIT_ID = $P{P_VISITID}]]></queryString>

