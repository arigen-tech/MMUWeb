	<queryString><![CDATA[SELECT EMPLOYEE_CATEGORY_NAME , ( CASE FROM_MONTH
        WHEN 1 THEN 'January'
        WHEN 2 THEN 'February'
        WHEN 3 THEN 'March'
        WHEN 4 THEN 'April'
        WHEN 5 THEN 'May'
        WHEN 6 THEN 'June'
        WHEN 7 THEN 'July'
        WHEN 8 THEN 'August'
        WHEN 9 THEN 'September'
        WHEN 10 THEN 'October'
        WHEN 11 THEN 'November'
        WHEN 12 THEN 'December'
       END ) FROM_MONTH , ( CASE TO_MONTH
        WHEN 1 THEN 'January'
        WHEN 2 THEN 'February'
        WHEN 3 THEN 'March'
        WHEN 4 THEN 'April'
        WHEN 5 THEN 'May'
        WHEN 6 THEN 'June'
        WHEN 7 THEN 'July'
        WHEN 8 THEN 'August'
        WHEN 9 THEN 'September'
        WHEN 10 THEN 'October'
        WHEN 11 THEN 'November'
        WHEN 12 THEN 'December'
       END )  TO_MONTH , case when upper (CATEGORY_DUE.STATUS)='Y'
then 'Active' else 'Inactive' end STATUS
FROM ship.CATEGORY_DUE
LEFT OUTER JOIN ship.VU_MAS_EMPLOYEE_CATEGORY ON VU_MAS_EMPLOYEE_CATEGORY.EMPLOYEE_CATEGORY_ID=CATEGORY_DUE.CATEGORY_ID
ORDER BY EMPLOYEE_CATEGORY_NAME]]></queryString>

