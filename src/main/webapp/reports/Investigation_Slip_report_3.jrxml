	<queryString><![CDATA[SELECT MAS_HOSPITAL.HOSPITAL_NAME ,DG_MAS_INVESTIGATION.INVESTIGATION_ID,PATIENT.SERVICE_NO ,PATIENT.EMPLOYEE_NAME ,
VU_MAS_RANK.RANK_NAME  , VU_MAS_ADMINISTRATIVE_SEX.ADMINISTRATIVE_SEX_NAME, PATIENT.PATIENT_NAME as PATIENT_NAME, 
age(current_date ,PATIENT.DATE_OF_BIRTH) , VU_MAS_RELATION.RELATION_NAME as RELATION_NAME,
DG_ORDER_HD.OTHER_INVESTIGATION , MAS_DEPARTMENT.DEPARTMENT_NAME ||' / '|| R1.RANK_NAME||'  '||U.FIRST_NAME AS DOCTOR_NAME
FROM SHIP.PATIENT 
LEFT OUTER JOIN SHIP.VISIT on PATIENT.PATIENT_ID=VISIT.PATIENT_ID
LEFT OUTER JOIN SHIP.VU_MAS_RELATION on VU_MAS_RELATION.RELATION_ID=PATIENT.RELATION_ID
LEFT OUTER JOIN SHIP.VU_MAS_RANK on VU_MAS_RANK.RANK_ID=PATIENT.RANK_ID
LEFT OUTER JOIN SHIP.VU_MAS_ADMINISTRATIVE_SEX on VU_MAS_ADMINISTRATIVE_SEX.ADMINISTRATIVE_SEX_ID=PATIENT.ADMINISTRATIVE_SEX_ID
LEFT OUTER JOIN SHIP.DG_ORDER_HD on DG_ORDER_HD.PATIENT_ID =PATIENT.PATIENT_ID 
LEFT OUTER JOIN SHIP.DG_ORDER_DT on DG_ORDER_DT.ORDERHD_ID=DG_ORDER_HD.ORDERHD_ID
LEFT OUTER JOIN SHIP.DG_MAS_INVESTIGATION on DG_MAS_INVESTIGATION.INVESTIGATION_ID=DG_ORDER_DT.INVESTIGATION_ID
LEFT OUTER JOIN SHIP.MAS_DEPARTMENT on VISIT.DEPARTMENT_ID=MAS_DEPARTMENT.DEPARTMENT_ID
LEFT OUTER JOIN SHIP.MAS_HOSPITAL on MAS_HOSPITAL.HOSPITAL_ID=VISIT.HOSPITAL_ID
LEFT OUTER JOIN SHIP.OPD_PATIENT_DETAILS on VISIT.VISIT_ID=OPD_PATIENT_DETAILS.VISIT_ID
LEFT OUTER JOIN SHIP.USERS U on OPD_PATIENT_DETAILS.DOCTOR_ID=U.USER_ID
LEFT OUTER JOIN SHIP.VU_MAS_EMPLOYEE E1 on E1.SERVICE_NO=U.SERVICE_NO
LEFT OUTER JOIN SHIP.VU_MAS_RANK R1 on R1.RANK_CODE=e1.RANK_ID
WHERE VISIT.VISIT_ID=$P{VISIT_ID}]]></queryString>

