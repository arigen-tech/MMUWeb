	</parameter>
	<parameter name="Level_of_user" isForPrompting="true" class="java.lang.String">
		<defaultValueExpression ><![CDATA[new String("")]]></defaultValueExpression>
	</parameter>
	<parameter name="Mmu_id" isForPrompting="true" class="java.lang.Integer">
		<defaultValueExpression ><![CDATA[new Integer(0)]]></defaultValueExpression>
	</parameter>
	<parameter name="User_id" isForPrompting="true" class="java.lang.Integer">
		<defaultValueExpression ><![CDATA[new Integer(0)]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[SELECT MAS_STORE_ITEM.PVMS_NO , MAS_STORE_ITEM.NOMENCLATURE , SUM((PATIENT_PRESCRIPTION_DT.TOTAL - COALESCE(PATIENT_PRESCRIPTION_DT.QTY_ISSUED,0))) AS QUANTITY,
(CASE WHEN $P{Mmu_id}= 0 THEN '' ELSE MMU.MMU_NAME END)HOSPITAL_NAME 
FROM PATIENT_PRESCRIPTION_DT 
LEFT OUTER JOIN MAS_STORE_ITEM on PATIENT_PRESCRIPTION_DT.ITEM_ID=MAS_STORE_ITEM.ITEM_ID
LEFT OUTER JOIN PATIENT_PRESCRIPTION_HD on PATIENT_PRESCRIPTION_DT.PRESCRIPTION_HD_ID=PATIENT_PRESCRIPTION_HD.PRESCRIPTION_HD_ID 
LEFT OUTER JOIN VISIT ON PATIENT_PRESCRIPTION_HD.VISIT_ID = VISIT.VISIT_ID
LEFT OUTER JOIN MAS_DEPARTMENT ON MAS_DEPARTMENT.DEPARTMENT_ID = VISIT.DEPARTMENT_ID
LEFT OUTER JOIN MAS_MMU MMU on MMU.MMU_ID=PATIENT_PRESCRIPTION_HD.MMU_ID
LEFT OUTER JOIN MAS_ITEM_TYPE on MAS_STORE_ITEM.ITEM_TYPE_ID =MAS_ITEM_TYPE.ITEM_TYPE_ID
where  PATIENT_PRESCRIPTION_HD.PRESCRIPTION_DATE>= $P{Fromdate} and PATIENT_PRESCRIPTION_HD.PRESCRIPTION_DATE<=$P{Todate}
AND (PATIENT_PRESCRIPTION_HD.MMU_ID = $P{Mmu_id} OR $P{Mmu_id}= 0) AND
	PATIENT_PRESCRIPTION_HD.STATUS ='I' AND 
	 (PATIENT_PRESCRIPTION_DT.TOTAL - PATIENT_PRESCRIPTION_DT.QTY_ISSUED)>0
	and ((PATIENT_PRESCRIPTION_DT.TOTAL - COALESCE(PATIENT_PRESCRIPTION_DT.QTY_ISSUED,0))) > 0
		AND     MMU.MMU_ID IN (
	  	SELECT 	MMU.MMU_ID
		FROM 	MAS_MMU MMU,(SELECT  	to_number(foo,'99999999') IDS
				     FROM 	regexp_split_to_table((SELECT 	(CASE WHEN 'M' = $P{Level_of_user} THEN substring(M.MMU_ID, 1 ,length(M.MMU_ID)-1) 
										      WHEN 'C' = $P{Level_of_user} THEN substring(M.CITY_ID, 1 ,length(M.CITY_ID)-1) 
										      WHEN 'D' = $P{Level_of_user} THEN substring(M.DISTRICT_ID, 1 ,length(M.DISTRICT_ID)-1) 
										      ELSE '1' END)
									FROM 	USERS M 
									WHERE 	M.USER_ID =$P{User_id}), E',') As foo)  ALL_ID 
		WHERE ( (MMU.MMU_ID=ALL_ID.IDS AND 'M' =$P{Level_of_user}) OR
	  		(MMU.CITY_ID=ALL_ID.IDS AND 'C' =$P{Level_of_user}) OR
			(MMU.STATE_ID=ALL_ID.IDS AND 'S' =$P{Level_of_user}) OR
			(MMU.DISTRICT_ID=ALL_ID.IDS AND 'D' =$P{Level_of_user}) ) 
		)
GROUP BY MAS_STORE_ITEM.PVMS_NO , MAS_STORE_ITEM.NOMENCLATURE,4
order by MAS_STORE_ITEM.NOMENCLATURE]]></queryString>

