	<queryString><![CDATA[SELECT IR.INJURY_DATE , PT.SERVICE_NO , PT.EMPLOYEE_NAME , (AGE(current_date ,PT.DATE_OF_BIRTH)) As AGE,
IR.ICD_DIAGNOSIS , IR.LETTER_NO , IR.LETTER_DATE ,IR.APPROVING_AUTHORITY_NAME ,IR.APPROVAL_DATE,IR.REMARK ,MAS_HOSPITAL.HOSPITAL_NAME 
FROM ship.INJURY_REGISTER IR
LEFT OUTER JOIN ship.PATIENT PT ON PT.PATIENT_ID=IR.PATIENT_ID
LEFT OUTER JOIN ship.MAS_HOSPITAL ON MAS_HOSPITAL.HOSPITAL_ID=IR.HOSPITAL_ID
WHERE CAST( IR.INJURY_DATE As Date) BETWEEN $P{FromDate} AND $P{ToDate}
AND MAS_HOSPITAL.HOSPITAL_ID =$P{HOSPITAL_ID} AND (upper(PT.SERVICE_NO)=upper($P{SERVICE_NO})  OR upper($P{SERVICE_NO})= ('0')) AND SERVICE_NO is not null $P!{query}
ORDER BY IR.INJURY_DATE DESC]]></queryString>

