	<parameter name="VISIT_ID" isForPrompting="true" class="java.lang.Integer">
		<defaultValueExpression ><![CDATA[new Integer(0)]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[SELECT MAS.DISPOSAL_NAME ,(CASE WHEN MAS.DISPOSAL_CODE ='011' THEN (CASE WHEN COALESCE(DI.DISPOSAL_DAYS,0)=0 THEN '' ELSE CAST((COALESCE(DI.DISPOSAL_DAYS,0)*24) As VARCHAR)END) ||' HOUR' ELSE (CASE WHEN COALESCE(DI.DISPOSAL_DAYS,0)=0 THEN '' ELSE CAST(COALESCE(DI.DISPOSAL_DAYS,0) AS VARCHAR) ||' DAYS' END) END)AS DISPOSAL_DAYS,
OPD.OTHER_DISPOSAL 
FROM ship.OPD_DISPOSAL_DETAILS DI
LEFT OUTER JOIN ship.MAS_DISPOSAL MAS on MAS.DISPOSAL_ID=DI.DISPOSAL_ID
LEFT OUTER JOIN ship.OPD_PATIENT_DETAILS OPD on OPD.VISIT_ID=DI.VISIT_ID
WHERE DI.VISIT_ID=$P{VISIT_ID}  
ORDER BY MAS.DISPOSAL_NAME]]></queryString>

