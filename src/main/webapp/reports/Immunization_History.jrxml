	<queryString><![CDATA[SELECT  NOMENCLATURE AS IMMUNIZATION_NAME,TO_CHAR(PRESCRIPTION_DATE, 'DD/MM/YYYY')PRESCRIPTION_DATE,TO_CHAR(IMMUNIZATION_DATE, 'DD/MM/YYYY')IMMUNIZATION_DATE,DURATION,TO_CHAR(NEXT_DUE_DATE, 'DD/MM/YYYY') NEXT_DUE_DATE FROM (
SELECT  PIH.IMMUNIZATION_DATE,MSI.NOMENCLATURE,PIH.PRESCRIPTION_DATE,PIH.DURATION,PIH.NEXT_DUE_DATE,DENSE_RANK() OVER (ORDER BY PIH.PRESCRIPTION_DATE  DESC)  AS AKS
FROM    ship.PATIENT_IMMUNIZATION_HISTORY PIH LEFT OUTER JOIN ship.MAS_STORE_ITEM MSI ON PIH.ITEM_ID=MSI.ITEM_ID
                                         LEFT OUTER JOIN ship.VISIT VT ON VT.VISIT_ID=PIH.VISIT_ID
WHERE   PIH.PATIENT_ID=$P{P_PATIENTID} AND UPPER(VT.VISIT_STATUS)='C')AKS
WHERE   AKS.AKS <=3]]></queryString>

