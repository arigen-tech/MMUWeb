		<defaultValueExpression ><![CDATA[new Integer(0)]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[SELECT  VT.VISIT_ID AS VISIT_ID,
        HD.PROCEDURE_TYPE AS NURSING_FLAG,
        (CASE HD.PROCEDURE_TYPE WHEN 'M' THEN 'MINOR SURGERY'
                               WHEN 'P' THEN 'PHYSIOTHEROPY'
                               ELSE 'NURSING CARE' END) NURSING_TYPE,
        NURSING_NAME AS NAME,
        TO_CHAR(HD.REQUISITION_DATE, 'DD/MM/YYYY') AS START_DATE,
        MF.FREQUENCY_NAME AS FREQUENCY,
        DT.NO_OF_DAYS ,
        DT.REMARKS,
        ANESTHESIA_NAME AS TYPEOF_ANEST
FROM    ship.PROCEDURE_DT DT
LEFT OUTER JOIN ship.PROCEDURE_HD HD ON DT.PROCEDURE_HD_ID=HD.PROCEDURE_HD_ID
LEFT OUTER JOIN ship.VISIT VT ON VT.VISIT_ID=HD.VISIT_ID
LEFT OUTER JOIN ship.MAS_NURSING_CARE MNC ON DT.PROCEDURE_ID=MNC.NURSING_ID
LEFT OUTER JOIN ship.MAS_FREQUENCY MF ON MF.FREQUENCY_ID=DT.FREQUENCY_ID
LEFT OUTER JOIN ship.MAS_ANESTHESIA MA ON MA.ANESTHESIA_ID=DT.ANESTHESIA_ID
WHERE    HD.PATIENT_ID=$P{P_PATIENTID} AND HD.PROCEDURE_TYPE = 'M' AND  UPPER(VT.VISIT_STATUS)='C'  AND  HD.VISIT_ID = $P{P_VISITID}]]></queryString>

