	<queryString><![CDATA[SELECT MAS_ICD.ICD_NAME||' ('||MAS_ICD.ICD_CODE||')' As diagnosis,  MAS_MEDICAL_CATEGORY.MEDICAL_CATEGORY_NAME||'('||
PATIENT_MEDICAL_CAT.SYSTEM||')'||PATIENT_MEDICAL_CAT.CATEGORY_TYPE||'MT'||' WEF '|| 
to_char(PATIENT_MEDICAL_CAT.CATEGORY_DATE ,'DD/MM/YYYY') As CATEGORY_DATE
FROM ship.PATIENT_MEDICAL_CAT
LEFT OUTER JOIN ship.MAS_ICD ON PATIENT_MEDICAL_CAT.ICD_ID=MAS_ICD.ICD_ID
LEFT OUTER JOIN ship.MAS_MEDICAL_CATEGORY ON MAS_MEDICAL_CATEGORY.MEDICAL_CATEGORY_ID=PATIENT_MEDICAL_CAT.MEDICAL_CATEGORY_ID
WHERE PATIENT_MEDICAL_CAT.VISIT_ID=$P{VISIT_ID} and MAS_ICD.ICD_NAME is not null 
 AND ship.PATIENT_MEDICAL_CAT.MB_STATUS=$P{PVISITSTATUS} 
AND PATIENT_MEDICAL_CAT.P_MED_CAT_ID is null AND PATIENT_MEDICAL_CAT.RECOMMEND_FLAG is null
]]></queryString>

