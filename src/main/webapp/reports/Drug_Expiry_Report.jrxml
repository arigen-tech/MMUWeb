	</parameter>
	<parameter name="ToDate" isForPrompting="true" class="java.util.Date">
		<defaultValueExpression ><![CDATA[new java.util.Date()]]></defaultValueExpression>
	</parameter>
	<parameter name="Level_of_user" isForPrompting="true" class="java.lang.String">
		<defaultValueExpression ><![CDATA[new String("")]]></defaultValueExpression>
	</parameter>
	<parameter name="User_id" isForPrompting="true" class="java.lang.Integer">
		<defaultValueExpression ><![CDATA[new Integer(0)]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[SELECT MAS_MMU.MMU_NAME HOSPITAL_NAME , MAS_DEPARTMENT.DEPARTMENT_NAME ,MAS_STORE_ITEM.PVMS_NO, MAS_STORE_ITEM.NOMENCLATURE,
MAS_STORE_UNIT.STORE_UNIT_NAME ,STORE_ITEM_BATCH_STOCK.BATCH_NO,
STORE_ITEM_BATCH_STOCK.EXPIRY_DATE,sum(STORE_ITEM_BATCH_STOCK.CLOSING_STOCK) as CLOSING_STOCK,
MAS_MMU.MMU_NAME
FROM STORE_ITEM_BATCH_STOCK 
LEFT OUTER JOIN MAS_STORE_ITEM  on STORE_ITEM_BATCH_STOCK.ITEM_ID = MAS_STORE_ITEM.ITEM_ID  
LEFT OUTER JOIN MAS_STORE_UNIT on MAS_STORE_ITEM.ITEM_UNIT_ID=MAS_STORE_UNIT.STORE_UNIT_ID
LEFT OUTER JOIN MAS_DEPARTMENT on STORE_ITEM_BATCH_STOCK.DEPARTMENT_ID=MAS_DEPARTMENT.DEPARTMENT_ID
LEFT OUTER JOIN MAS_MMU on STORE_ITEM_BATCH_STOCK.MMU_ID=MAS_MMU.MMU_ID
WHERE (STORE_ITEM_BATCH_STOCK.DEPARTMENT_ID =$P{DEPARTMENT_ID} OR $P{DEPARTMENT_ID} =0)
and STORE_ITEM_BATCH_STOCK.CLOSING_STOCK > 0
AND
	 MAS_MMU.MMU_ID IN (
	  	SELECT 	MMU.MMU_ID
		FROM 	MAS_MMU MMU,CITY_MMU_MAPPING MCM,(SELECT  	to_number(foo,'99999999') IDS
				     FROM 	regexp_split_to_table((SELECT 	(CASE WHEN 'M' = $P{Level_of_user} THEN substring(M.MMU_ID, 1 ,length(M.MMU_ID)-1) 
										      WHEN 'C' = $P{Level_of_user} THEN substring(M.CITY_ID, 1 ,length(M.CITY_ID)-1) 
										      WHEN 'D' = $P{Level_of_user} THEN substring(M.DISTRICT_ID, 1 ,length(M.DISTRICT_ID)-1) 
										      ELSE '1' END)
									FROM 	USERS M 
									WHERE 	M.USER_ID =$P{User_id}), E',') As foo)  ALL_ID 
		WHERE 	MCM.MMU_ID = MMU.MMU_ID AND
			( (MMU.MMU_ID=ALL_ID.IDS AND 'M' =$P{Level_of_user}) OR
	  		(MCM.CITY_ID=ALL_ID.IDS AND 'C' =$P{Level_of_user}) OR
			(MMU.STATE_ID=ALL_ID.IDS AND 'S' =$P{Level_of_user}) OR
			(MMU.DISTRICT_ID=ALL_ID.IDS AND 'D' =$P{Level_of_user}) ) 
		)			
and (STORE_ITEM_BATCH_STOCK.MMU_ID =$P{HOSPITAL_ID} OR $P{HOSPITAL_ID} =0)
and STORE_ITEM_BATCH_STOCK.EXPIRY_DATE::date between $P{FromDate} and $P{ToDate}
$P!{query}
group by MAS_MMU.MMU_NAME , MAS_DEPARTMENT.DEPARTMENT_NAME ,MAS_STORE_ITEM.PVMS_NO ,
MAS_STORE_ITEM.NOMENCLATURE,MAS_STORE_UNIT.STORE_UNIT_NAME ,STORE_ITEM_BATCH_STOCK.BATCH_NO,
STORE_ITEM_BATCH_STOCK.EXPIRY_DATE,(STORE_ITEM_BATCH_STOCK.CLOSING_STOCK)
order by MAS_MMU.MMU_NAME,MAS_STORE_ITEM.NOMENCLATURE,STORE_ITEM_BATCH_STOCK.EXPIRY_DATE]]></queryString>

