	<parameter name="Current_Date" isForPrompting="false" class="java.util.Date">
		<defaultValueExpression ><![CDATA[new Date()]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[SELECT 	item.pvms_no,
        item.NOMENCLATURE,
        RIT.closing_stock,
        RIT.mmu_name
FROM    ( 
SELECT 	mmu_name,store_issue_m.mmu_id,store_issue_t.item_id,sum(store_issue_t.qty_issued) qty_issued,AKS.closing_stock 
FROM 	store_issue_m inner join store_issue_t ON store_issue_m.id=store_issue_t.issue_m_id 
					  inner join Mas_mmu mmu ON mmu.mmu_id=store_issue_m.mmu_id 
					  inner join (select mmu_id,item_id,sum(closing_stock) closing_stock from store_item_batch_stock  where mmu_id is not null group by mmu_id,item_id) AKS 
					  ON AKS.mmu_id=store_issue_m.mmu_id AND  AKS.item_id=store_issue_t.item_id	
where 	store_issue_t.issue_date::date between current_date-7 and current_date AND store_issue_m.mmu_id=$P{MMU_ID}
Group by mmu_name,store_issue_m.mmu_id,store_issue_t.item_id,AKS.closing_stock
		)RIT,
		Mas_store_item item
WHERE   item.item_id=RIT.item_id AND (RIT.qty_issued*2)-RIT.closing_stock >0	
ORDER BY   NOMENCLATURE;
]]></queryString>

