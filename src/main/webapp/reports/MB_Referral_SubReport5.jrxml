	<queryString><![CDATA[SELECT VU_MAS_RANK.RANK_NAME||' '||USERS.FIRST_NAME AS doctor_name
FROM  ship.OPD_PATIENT_DETAILS
LEFT OUTER JOIN ship.REFERRAL_PATIENT_HD on REFERRAL_PATIENT_HD.OPD_PATIENT_DETAILS_ID=OPD_PATIENT_DETAILS.OPD_PATIENT_DETAILS_ID
LEFT OUTER JOIN ship.REFERRAL_PATIENT_DT on REFERRAL_PATIENT_HD.REFREAL_HD_ID=REFERRAL_PATIENT_DT.REFREAL_HD_ID
LEFT OUTER JOIN ship.MAS_ICD on MAS_ICD.ICD_ID=REFERRAL_PATIENT_DT.DIAGNOSIS_ID
LEFT OUTER JOIN ship.USERS on USERS.USER_ID=OPD_PATIENT_DETAILS.DOCTOR_ID
LEFT OUTER JOIN ship.VU_MAS_EMPLOYEE on VU_MAS_EMPLOYEE.SERVICE_NO=USERS.SERVICE_NO
LEFT OUTER JOIN ship.VU_MAS_RANK on VU_MAS_RANK.RANK_CODE=VU_MAS_EMPLOYEE.RANK_ID
WHERE REFERRAL_PATIENT_HD.REFREAL_HD_ID=$P{REFREAL_HD_ID}
GROUP BY VU_MAS_RANK.RANK_NAME||' '||USERS.FIRST_NAME]]></queryString>

