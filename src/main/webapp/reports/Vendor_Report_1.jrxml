	<queryString>
		<![CDATA[select MMV.mmu_Vendor_name,md.district_name,mc.city_name,
(SELECT STRING_AGG(MMU_NAME, ', ') MMU FROM capture_vendor_bill_mmu_detail CVBMD,MAS_MMU mmu WHERE CVBMD.capture_vendor_bill_detail_id = c.capture_vendor_bill_detail_id and CVBMD.mmu_id = mmu.mmu_id) mmu,
bill_month+1||'/'||bill_year bill_dt,c.invoice_date,c.invoice_amount,c.invoice_no,C.penalty_amount,c.phase
from capture_vendor_bill_detail c left outer join vendor_invoice_approval via on via.capture_vendor_bill_detail_id =c.capture_vendor_bill_detail_id  and authority_id =1 and authority_action='A'
left outer join mas_mmu_Vendor MMV on MMV.mmu_Vendor_id = C.vendor_id ,mas_district md,mas_city mc
where c.capture_vendor_bill_detail_id  =$P{detail_id} and
md.district_id=	c.district_id and
mc.city_id =c.city_id]]>
	</queryString>
