	<queryString><![CDATA[SELECT ORDER_PURPOSE , QUATATION_OF_ITEM , REF_OF_GOVT_AUTHORITY ,ISSUED_UNDER, PAYING_AGENCY, VIDE_NOTE_NUMBER,UO_NO,  REMARKS , ORDER_DATE,
COMM_WITH_SANC,SANC_OVERRULING , MAS_STORE_SUPPLIER.SUPPLIER_NAME||'/'||MAS_STORE_SUPPLIER.ADDRESS1, ORDER_VALUE as Value_of_sanction,
BOOKED_BUDGET,APPROVED_BY As APPROVED_BY1,R1.RANK_NAME||' '||USERS1.FIRST_NAME AS CREATED_BY,R2.RANK_NAME||' '||USERS2.FIRST_NAME AS APPROVED_BY,APPROVED_DATE,
(case upper (STORE_SO_M.STATUS) when  'A' then 'Approved By :'
                                        WHEN 'R' THEN 'Rejected By:'
                                        else '' end) As BYNAME,
(case upper (STORE_SO_M.STATUS) when  'A' then 'Approved Date'              
                                        WHEN 'R' THEN 'Rejected Date'
                                        else '' end) As BYDATE1
FROM ship.STORE_SO_M
LEFT OUTER JOIN ship.STORE_SO_T ON STORE_SO_T.SO_M_ID=STORE_SO_M.SO_M_ID
LEFT OUTER JOIN ship.MAS_STORE_SUPPLIER ON MAS_STORE_SUPPLIER.SUPPLIER_ID=STORE_SO_M.SUPPLIER_ID
LEFT OUTER JOIN ship.USERS USERS1 ON USERS1.USER_ID=STORE_SO_M.CREATED_BY
LEFT OUTER JOIN ship.VU_MAS_EMPLOYEE E1 on E1.SERVICE_NO=USERS1.SERVICE_NO
LEFT OUTER JOIN ship.VU_MAS_RANK R1 on R1.RANK_CODE=E1.RANK_ID
LEFT OUTER JOIN ship.USERS USERS2 ON USERS2.USER_ID=STORE_SO_M.APPROVED_BY
LEFT OUTER JOIN ship.VU_MAS_EMPLOYEE E2 on E2.SERVICE_NO=USERS2.SERVICE_NO
LEFT OUTER JOIN ship.VU_MAS_RANK R2 on R2.RANK_CODE=E2.RANK_ID
WHERE STORE_SO_M.SO_M_ID=$P{SO_M_ID}]]></queryString>

