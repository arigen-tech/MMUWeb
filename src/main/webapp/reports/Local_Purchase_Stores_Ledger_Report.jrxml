HD.AUTHORIZED_ME_SCALE , HD.AUTHORIZED_QTY ,
DT.RECEIVED_NUMBER AS RV_NO , DT.RECEIVED_DATE , DT.BOARD_OUT 
FROM ship.APP_EQUIPMENT_HD HD
LEFT OUTER JOIN ship.APP_AUDIT_EQUIPMENT_DT ADT ON ADT.EQUIPMENT_HD_ID=HD.EQUIPMENT_HD_ID
