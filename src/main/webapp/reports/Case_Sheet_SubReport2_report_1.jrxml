		<defaultValueExpression ><![CDATA[new Integer(0)]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[SELECT MAS_ICD.ICD_NAME ,MAS_MEDICAL_CATEGORY.MEDICAL_CATEGORY_NAME,(CASE WHEN REFERRAL_PATIENT_HD.TREATMENT_TYPE ='E' THEN  EXT_DEPARTMENT ELSE MAS_DEPARTMENT.DEPARTMENT_NAME END) Speciality,
(CASE WHEN REFERRAL_PATIENT_HD.TREATMENT_TYPE ='E' THEN  EMPANELLED_HOSPITAL_NAME ELSE HOSPITAL_NAME END) HOSPITAL_NAME,INSTRUCTION
FROM public.OPD_PATIENT_DETAILS
LEFT OUTER JOIN public.REFERRAL_PATIENT_HD on REFERRAL_PATIENT_HD.OPD_PATIENT_DETAILS_ID=OPD_PATIENT_DETAILS.OPD_PATIENT_DETAILS_ID
LEFT OUTER JOIN public.REFERRAL_PATIENT_DT on REFERRAL_PATIENT_HD.REFREAL_HD_ID=REFERRAL_PATIENT_DT.REFREAL_HD_ID
LEFT OUTER JOIN public.PATIENT ON OPD_PATIENT_DETAILS.PATIENT_ID=PATIENT.PATIENT_ID
LEFT OUTER JOIN public.MAS_MEDICAL_CATEGORY ON MAS_MEDICAL_CATEGORY.MEDICAL_CATEGORY_ID=PATIENT.MEDICAL_CATEGORY_ID
LEFT OUTER JOIN public.MAS_ICD on MAS_ICD.ICD_ID=REFERRAL_PATIENT_DT.DIAGNOSIS_ID 
LEFT OUTER JOIN public.MAS_DEPARTMENT on MAS_DEPARTMENT.DEPARTMENT_ID=REFERRAL_PATIENT_DT.INT_DEPARTMENT_ID 
LEFT OUTER JOIN public.MAS_HOSPITAL on MAS_HOSPITAL.HOSPITAL_ID=REFERRAL_PATIENT_HD.INT_HOSPITAL_ID 
LEFT OUTER JOIN public.MAS_EMPANELLED_HOSPITAL on MAS_EMPANELLED_HOSPITAL.EMPANELLED_HOSPITAL_ID=REFERRAL_PATIENT_HD.EXT_HOSPITAL_ID 
WHERE REFERRAL_PATIENT_HD.REFREAL_HD_ID=$P{REFREAL_HD_ID}
ORDER BY ICD_NAME]]></queryString>

